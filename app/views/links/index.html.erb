<div class="container">
  <h1>Submit a New Link</h1>

  <%= form_for(@link) do |f| %>
    <%= f.text_field :title, placeholder: "Title", class:"form-control" %><br>
    <%= f.text_field :url, placeholder: "URL", class:"form-control" %><br>
    <%= f.submit "Save Link", class:"btn btn-primary"%>
  <% end %>
<div id="baby-fake-button">
  <p>Looks Like a button</p>
</div>

  <div class="fake-form">
    <input type="text" id="fake-form-title" value="" placeholder="title">
    <input type="text" id="fake-form-url" value=""placeholder="url">
  </div>

  <h1>My Links</h1>
    <div id="links-div">
      <table id="links-table">

      </table>
    </div>
</div>
