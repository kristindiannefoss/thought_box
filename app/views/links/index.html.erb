<div class="container">
  <h1>Submit a New Link</h1>

  <%= form_for(@link) do |f| %>
  <%= f.text_field :title, placeholder: "Title", class:"form-control" %><br>
  <%= f.password_field :url, placeholder: "URL", class:"form-control" %><br>
  <%= f.submit "Create Account", class:"btn btn-primary"%>

  <% end %>

  <h1>My Links</h1>

</div>
